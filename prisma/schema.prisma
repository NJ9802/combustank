// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"

  url = env("DATABASE_URL")

  directUrl = env("DIRECT_URL")
}

model Tank {
  id String @id @default(cuid())

  name String @unique

  averageLength Float

  averageHeight Float

  capacity Float

  measurement Float

  stock Float

  adminId String
}
